        <Label Name="LabelText" Content="" HorizontalAlignment="Left" Margin="90,190,0,0" VerticalAlignment="Top"/>
        <DataGrid Name="Grid1" HorizontalAlignment="Left" Height="285" Margin="30,80,0,0" VerticalAlignment="Top" Width="365" AutoGenerateColumns="True"/>
        <TextBox Name="TExtes" HorizontalAlignment="Left" Height="65" Margin="490,60,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="175"/>

    </Grid>
